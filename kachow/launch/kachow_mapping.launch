<launch>
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    <rosparam>
      base_frame: base_link
      map_frame: map
      odom_frame: odom
      map_update_interval: 0.3
      maxUrange: 0.5
      maxRange: 0.65
      sigma: 0.05
      kernelSize: 1
      lstep: 0.05
      astep: 0.05
      iterations: 5
      lsigma: 0.075
      ogain: 3.0
      lskip: 0
      minimumScore: 100
      linearUpdate: 0.025
      angularUpdate: 0.01745
      temporalUpdate: -1.0
      resampleThreshold: 0.5
      particles: 30
      xmin: -1.0
      ymin: -1.0
      xmax: 1.0
      ymax: 1.0
      delta: 0.05
      llsamplerange: 0.01
      llsamplestep: 0.01
      lasamplerange: 0.005
      lasamplestep: 0.005
      transform_publish_period: 0.5
    </rosparam>
  </node>
</launch>
