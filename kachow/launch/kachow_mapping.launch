<launch>
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    <rosparam>
      base_frame: base_footprint
      odom_frame: odom
      map_update_interval: 0.5
      maxUrange: 1.0
      maxRange: 1.0
      sigma: 0.05
      kernelSize: 1
      lstep: 0.05
      astep: 0.05
      iterations: 5
      lsigma: 0.075
      ogain: 3.0
      lskip: 0
      minimumScore: 30
      linearUpdate: 0.05
      angularUpdate: 0.0436
      temporalUpdate: -1.0
      resampleThreshold: 0.5
      particles: 8
      xmin: -1.0
      ymin: -1.0
      xmax: 1.0
      ymax: 1.0
      delta: 0.025
      llsamplerange: 0.01
      llsamplestep: 0.01
      lasamplerange: 0.005
      lasamplestep: 0.005
    </rosparam>
  </node>
</launch>

